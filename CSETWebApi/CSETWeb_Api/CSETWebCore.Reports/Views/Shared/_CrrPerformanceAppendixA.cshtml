@using CSETWebCore.Reports.Models
@using CSETWebCore.Model.Edm
@model CrrViewModel

<div class="container">
    <h1 style="font-family: Verdana, sans-serif">CRR Performance</h1>
    @{
        void RenderScores()
        {
            for (int i = 0; i < Model.ParentScores.Count; i++)
            {
                List<EDMscore> children = Model.ParentScores[i].children;
                <p><b>@Model.ParentScores[i].parent.Title_Id</b></p>
                for (int j = 0; j < children.Count; j++)
                {
                    <p>@string.Format("Title_Id: {0}, Color: {1}", children[j].Title_Id, children[j].Color)</p>
                }
            }

        }

        RenderScores();
    }
</div>

