@model string

@{ 
    string ToFull(string abb)
    {
        switch (abb)
        {
            case "Y":
                return "Yes";
            case "N":
                return "No";
            case "Inc":
            case "I":
                return "Incomplete";
            case "U":
                return "Unanswered";
            case "A":
                return "Alternate";
            case "NA":
                return "N/A";
            default:
                return abb;
        }
    }
}

@ToFull(Model)