<style>
    //EDM CHART COMPONENETS
    .triple-bar-chart {
        display: flex;
        flex-basis: 100%;
        flex-direction: column;
        width: 100%;
        height: 100%;
        position: relative;
    }

    .tbc-title {
        font-size: .7rem;
        font-weight: 600;
    }

    .tbc-bars {
        display: flex;
        width: 90%;
        flex-basis: 100%;
        flex-direction: row;
        height: 100%;
        align-self: center;
        padding-bottom: .2em;
        margin-top: 1.3em;
        position: relative;
        bottom: 0em;
    }

    .tbc-bar {
        display: flex;
        flex-basis: 100%;
        margin: 0 0.1em;
        align-self: flex-end;
        justify-content: center;
        position: relative;
        bottom: 0em;
    }

    .tbc-bar-label {
        position: relative;
        bottom: 1.3em;
        color: black !important;
    }

    .tbc-bottom-percent {
        display: flex;
        width: 100%;
        align-self: center;
        border-top: solid 1px black;
        padding: 0 5%;
    }

    .edm-horizontal-bar-chart {
        display: flex;
        width: 100%;
    }

    .edm-hzbc-bar {
        display: flex;
        width: 100%;
        // font-size: 10px;
    }

        .edm-hzbc-bar div {
            display: flex;
            flex-basis: 100%;
            justify-content: center;
        }

    :host {
        --edm-aa-col-one-width: 55px;
        --edm-aa-col-two-width: 180px;
        --edm-aa-col-three-width: 180px;
        --edm-aa-table-margin: 2px;
        --edm-aa-page-height: 66em;
    }




    .edm-aa-section {
        display: flex;
        flex-direction: column;
        padding: 2em 0;
        height: var(--edm-aa-page-height)
    }

    .edm-aa-headers {
        display: flex;
    }

    .edm-aa-header {
        background-color: black;
        color: white;
        padding-left: 3px;
        font-size: 0.7em;
        font-weight: 700;
    }

    .edm-body-col-1, .edm-body-col-2, .edm-body-col-3, .edm-aa-left-id {
        margin-right: .2em;
    }

    .edm-body-col-1 {
        display: flex;
        width: var(--edm-aa-col-one-width);
    }

    .edm-body-col-2 {
        display: flex;
        width: var(--edm-aa-col-two-width);
    }

    .edm-body-col-3 {
        display: flex;
        width: var(--edm-aa-col-three-width);
    }

    .edm-body-col-4 {
        display: flex;
        flex-grow: 1;
    }

    .edm-aa-body-split {
        display: flex;
        margin-top: var(-edm-aa-table-margin);
        flex-basis: 100%;
    }

    .edm-aa-left-id {
        display: flex;
        color: white;
        writing-mode: tb-rl;
        transform: rotate(-180deg);
        justify-content: center;
        padding-right: 1em;
        margin-top: 4px;
        font-weight: 600;
    }

    .edm-aa-right-content {
        display: flex;
        width: calc(100% - var(--edm-aa-col-one-width));
        flex-direction: column;
    }

    .edm-aa-body {
        flex-direction: column;
        display: flex;
        flex-basis: 100%;
        margin-top: 3px;
    }

    .edm-aa-body-category-section {
        display: flex;
        margin-top: 5px;
        border-top: solid black 1px;
    }

    .edm-aa-body-function-summary {
        display: flex;
        flex-grow: 1;
        font-weight: 700;
    }

    .edm-aa-body-function-graph {
        display: flex;
        width: 100%;
        height: 120px;
    }

    .edm-aa-body-category {
        width: var(--edm-aa-col-two-width);
    }

    .edm-aa-body-category-text {
        padding: 0 12px;
    }

    .edm-aa-body-subcategories {
        display: flex;
        flex-direction: column;
        flex-grow: 1;
    }

    .edm-aa-function-summary {
        min-height: 100px;
        border: none
    }

    .edm-aa-body-subcategory {
        flex-grow: 1;
        display: flex;
    }

        .edm-aa-body-subcategory:not(:last-child) {
            border-bottom: solid black .1px;
        }

</style>

